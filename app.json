{
  "expo": {
    "name": "Pokémon Pokédex",
    "slug": "pokemon-pokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pokemonpokedex",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDD835"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pokemonpokedex.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FDD835"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pokemonpokedex.app",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#FDD835",
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "codePush": {
        "enabled": true,
        "androidDeploymentKey": "YOUR_ANDROID_DEPLOYMENT_KEY",
        "iosDeploymentKey": "YOUR_IOS_DEPLOYMENT_KEY"
      },
      "appCenter": {
        "android": {
          "appSecret": "f23d6114-d92a-474c-b32f-a7aff8d0153e"
        },
        "ios": {
          "appSecret": "YOUR_IOS_APP_SECRET_HERE"
        }
      },
      "router": {},
      "eas": {
        "projectId": "d55360b2-bd98-4188-906a-960521a3d7d5"
      }
    }
  }
}